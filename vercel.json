{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/images/**",
      "use": "'@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/",
      "dest": "public/images/**.jpg"
    }
  ]
}
